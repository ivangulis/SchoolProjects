package table;

import javafx.beans.property.DoubleProperty;
import javafx.beans.property.SimpleDoubleProperty;
import javafx.beans.property.SimpleStringProperty;
import javafx.beans.property.StringProperty;

public class TableChampion {
	
	private final StringProperty name;
	private final StringProperty role;
	private final StringProperty winRatio;

	public TableChampion(String name, String role, Double winRatio) {
		this.name = new SimpleStringProperty(name);
		this.role = new SimpleStringProperty(role);
		this.winRatio = new SimpleStringProperty(winRatio);
	}

	public StringProperty getName() {
		return name;
	}

	public StringProperty getRole() {
		return role;
	}

	public StringProperty getWinRatio() {
		return winRatio;
	}
	
	

}
